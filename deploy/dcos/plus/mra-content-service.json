{
  "id": "/content-service",
  "instances": 1,
  "cpus": 0.5,
  "mem": 512,
  "disk": 0,
  "gpus": 0,
  "fetch": [
    {
      "uri": "<IMAGE_REPOSITORY_CREDENTIALS>",
      "extract": true,
      "executable": false,
      "cache": false
    }
  ],
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "<IMAGE_REPO_AND_TAG>",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 443,
          "hostPort": 0,
          "servicePort": 10000,
          "protocol": "tcp",
          "name": "default"
        }
      ],
      "privileged": false,
      "forcePullImage": true
    }
  },
  "upgradeStrategy": {
    "minimumHealthCapacity": 1,
    "maximumOverCapacity": 1
  },
  "unreachableStrategy": {
    "inactiveAfterSeconds": 300,
    "expungeAfterSeconds": 600
  },
  "killSelection": "YOUNGEST_FIRST",
  "requirePorts": true,
  "env": {
    "NETWORK": "fabric",
    "RETHINKDB_URL": "content-db.marathon.mesos:28015"
  }
}
